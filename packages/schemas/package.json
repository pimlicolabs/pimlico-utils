{
    "name": "@pimlico/schemas",
    "version": "0.0.1",
    "author": "Pimlico",
    "main": "./_cjs/src/index.js",
    "module": "./_esm/src/index.js",
    "types": "./src/index.ts",
    "typings": "./src/index.ts",
    "type": "module",
    "sideEffects": false,
    "description": "Zod schemas and utilities related to Account Abstraction. Made by https://pimlico.io",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "pnpm build:cjs && pnpm build:esm",
        "build:cjs": "tsc --project ./../../tsconfig/tsconfig.schemas.cjs.json && tsc-alias -p ./../../tsconfig/tsconfig.schemas.cjs.json && printf '{\"type\":\"commonjs\"}' > ./_cjs/package.json",
        "build:esm": "tsc --project ./../../tsconfig/tsconfig.schemas.esm.json && tsc-alias -p ./../../tsconfig/tsconfig.schemas.esm.json && printf '{\"type\": \"module\",\"sideEffects\":false}' > ./_esm/package.json"
    },
    "devDependencies": {
        "typescript": "^5.7.2",
        "viem": "^2.21.52",
        "zod": "^4.0.10"
    },
    "peerDependencies": {
        "viem": "^2.21.52",
        "zod": "^4.0.0"
    }
}
